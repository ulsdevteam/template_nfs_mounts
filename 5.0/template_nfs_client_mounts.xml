                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>last(/Template NFS Client Mounts/nfs.server.check[{#SERVER}])&lt;&gt;0</expression>
                            <name>NFS connection to server {#SERVER} failed</name>
                            <priority>3</priority>
