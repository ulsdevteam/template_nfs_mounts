                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>last(/Template NFS Client Mounts/nfs.mount.check[{#MOUNTPOINT}])&lt;&gt;0</expression>
                            <name>NFS mount {#MOUNTPOINT} is not accessible</name>
                            <priority>3</priority>
